{
  "name": "scaffold-node-api",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "build": "pnpm build:fastify",
    "build:fastify": "tsup src --out-dir build",
    "dev": "pnpm start:dev",
    "format": "pnpm biome format .",
    "format:fix": "pnpm biome format --write .",
    "lint": "pnpm biome lint .",
    "lint:fix": "pnpm biome lint --apply .",
    "lint:fix:force": "pnpm biome lint --apply-unsafe .",
    "start": "pnpm start:fastify",
    "start:dev": "pnpm start:dev:fastify",
    "start:dev:fastify": "tsx watch ./src/infra/http/web/fastify",
    "start:fastify": "node ./build/infra/http/web/fastify",
    "test": "pnpm exec vitest"
  },
  "keywords": [],
  "author": "Jump Source",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "1.6.8",
    "dotenv": "16.4.5",
    "fastify": "^4.26.2",
    "zod": "3.23.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.1",
    "@types/node": "20.12.7",
    "sinon": "17.0.1",
    "tsup": "8.0.2",
    "tsx": "4.7.3",
    "typescript": "5.4.5",
    "vitest": "1.5.2"
  }
}
